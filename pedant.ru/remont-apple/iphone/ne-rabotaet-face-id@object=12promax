<script>
    document.addEventListener("DOMContentLoaded", function(event) {
        (function($) {

            function loadBlocks() {
                var blocks = $('.ajax-block[data-ajax]:not(".loaded"):not(".loading")');
                var step = 0;

                blocks.each(function() {
                    var _block = $(this);
                    var _settings = _block.data('ajax');
                    $.ajax({
                        url: _settings.url,
                        type: 'POST',
                        data: _settings.params,
                        dataType: 'HTML',
                        beforeSend: function() {
                            _block.addClass('loading');
                        },
                        success: function(data) {
                            _block.append(data);
                            _block.removeClass('loading');
                            _block.addClass('loaded');
                            step++;

                            // после последнего блока запускаем событие
                            if (step == blocks.length)
                                $(document.body).trigger('blocksloaded');
                        }
                    })
                });

                if (blocks.length == 0)
                    $(document.body).trigger('blocksloaded');
            }

            loadBlocks();

            $(document.body).on('pageloaded', function() {
                loadBlocks();
            });

        })(jQuery);

    });
</script>

