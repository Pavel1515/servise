<script>
    function SmartFirstScreen() {
        var htmlElement = document.querySelector('.section.first-screen');

        if (!htmlElement) return;

        var rightSide = htmlElement.querySelector('.side.right');
        var leftSide = htmlElement.querySelector('.side.left');
        var background = htmlElement.querySelector('.background');

        if (window.matchMedia("(max-width: 767.98px)").matches) {
            rightSide.style.height = (document.body.clientWidth * 0.84) / 16 + 'rem';
            background.style.top = (leftSide.clientHeight) / 16 + 'rem';
        } else {
            rightSide.style.height = 'auto';
            background.style.top = "";
        }
    }

    SmartFirstScreen();

    window.addEventListener("resize", function () {
        SmartFirstScreen();
    })
</script><div class='section five-reasons' id='five-reasons'>
