<script>
    $(document).ready(function() {
        if (window.Page.theme !== '' && window.Page.group === '') {
            $('.call-methods-widget').addClass('theme');
        } else if (window.Page.group !== '' && window.Page.object === '') {
            $('.call-methods-widget').addClass('objectgroup');
        } else if (window.Page.group !== '' && window.Page.service !== '') {
            $('.call-methods-widget').addClass('theme-new');
        } else if (window.Page.object !== '' && window.Page.service === '' && window.Page.problem === '') {
            $('.call-methods-widget').addClass('object');
        } else if (window.Page.object !== '' && window.Page.problem !== '') {
            $('.call-methods-widget').addClass('serviceobjectgroup');
        }
    });
</script>

